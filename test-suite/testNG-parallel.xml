        <parameter name="email" value="karishma.barot+1@techholding.co"></parameter>
        <parameter name="device" value="Theragun Mini"></parameter>
        <classes>
            <class name="testCases.LoginTCs"></class>
            <class name="testCases.NonBLECommanTCs"></class>
        </classes>
    </test>


</suite>

