<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Testing Therabody App" parallel="tests" thread-count="2">
    <test name="Non BLE Flows by Device Name: WaveSeries Roller">
        <parameter name="platform" value="Android"/>

        <parameter name="email" value="karishma.barot@techholding.co"></parameter>
